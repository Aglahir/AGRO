int i, j;
int mat1[3][3];
float mat2[3][3];

class Math{
    + int a;
    - int b;
}

class MathSon : Math{
    + int c;
}

void printMat(int matN) {
    for (i = 0; i < 3; i = i + 1;)
    {
        for (j = 0; j < 3; j = j + 1;)
        {
            if (matN == 1){
                print(mat1[i][j]);
            }
            else {
                print(mat2[i][j]);
            }
        }
    }
}

int owo(int toAssign){
    Math a;
    a.a = toAssign;
    return a.a;
}

main {
    
    for (i = 1; i < 4; i = i + 1;)
    {
        for (j = 1; j < 4; j = j + 1;)
        {
            %% print(i,j,"---");
            mat1[i-1][j-1] = i * j;
            mat2[i-1][j-1] = i / j;
        }
    }

    printMat(1);
    printMat(2);

    print(mat1[1][1]);
    print(mat1[1][2]);

    Math a;
    a.a = 5;
    print(a.a);

    print(owo(10) + a.a);

    MathSon b;
    b.a = 3;
    b.c = 5;
    print(b.a, b.c);
}